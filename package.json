{
  "name": "@kamaalio/joi-to-zod-codemod",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "joi-to-zod-codemod": "./bin/run.mjs"
  },
  "files": [
    "dist",
    "bin",
    "oclif.manifest.json"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:u": "vitest run --update",
    "build": "pnpm run compile && pnpm run prepack",
    "clean:build": "rm -rf dist tsconfig.tsbuildinfo && pnpm run build",
    "compile": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint --max-warnings 0",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif manifest"
  },
  "repository": "kamaal111/joi-to-zod-codemod",
  "bugs": "https://github.com/kamaal111/joi-to-zod-codemod/issues",
  "keywords": [],
  "author": "Kamaal Farah",
  "license": "MIT",
  "packageManager": "pnpm@10.30.3",
  "dependencies": {
    "@ast-grep/napi": "^0.41.0",
    "@kamaalio/codemod-kit": "^0.0.42",
    "@kamaalio/kamaal": "^0.8.0",
    "@oclif/core": "^4.8.1",
    "@oclif/plugin-help": "^6.2.37"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@kamaalio/prettier-config": "^0.1.2",
    "@oclif/test": "^4.1.16",
    "@types/node": "^25.3.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "joi": "^18.0.2",
    "lint-staged": "^16.2.7",
    "oclif": "^4.22.81",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "oclif": {
    "bin": "joi-to-zod-codemod",
    "dirname": "joi-to-zod-codemod",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {
      "run": {
        "description": "Run codemod"
      }
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "prettier": "@kamaalio/prettier-config"
}
